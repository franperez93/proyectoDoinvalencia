{% extends 'base.html.twig' %}

{% block body %}
    <h1>tipo_Evento list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tipo_Evento in tipo_Eventos %}
            <tr>
                <td><a href="{{ path('tipo_evento_show', { 'id': tipo_Evento.id }) }}">{{ tipo_Evento.id }}</a></td>
                <td>{{ tipo_Evento.nombre }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('tipo_evento_show', { 'id': tipo_Evento.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('tipo_evento_edit', { 'id': tipo_Evento.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipo_evento_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
