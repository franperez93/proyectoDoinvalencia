{% extends 'base.html.twig' %}

{% block body %}
    <h1>Participaciones</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ participacione.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if participacione.fecha %}{{ participacione.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Usuarioid</th>
                <td>{{ participacione.usuarioId }}</td>
            </tr>
            <tr>
                <th>Eventoid</th>
                <td>{{ participacione.eventoId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('participaciones_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('participaciones_edit', { 'id': participacione.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
